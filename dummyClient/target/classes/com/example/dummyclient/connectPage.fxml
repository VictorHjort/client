<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dummyclient.ConnectPageController">
    <ImageView fitHeight="600.0" fitWidth="1080.0" pickOnBounds="true">
        <Image url="https://media.discordapp.net/attachments/889864141534285864/902528602619387904/JeopartyBG.png?width=1135&amp;height=638"/>
    </ImageView>
    <Button fx:id="connectButton" alignment="CENTER" contentDisplay="CENTER" layoutX="416.0" layoutY="405.0" onAction="#connectButtonPressed" prefHeight="66.0" prefWidth="249.0" style="-fx-background-color: #FFF6E2;" text="Connect" textFill="#944110">
        <font>
            <Font name="Arial Bold" size="30.0" />
        </font>
        <effect>
            <DropShadow offsetY="10.0">
                <color>
                    <Color opacity="0.4000000059604645" />
                </color></DropShadow>
        </effect>
    </Button>
    <TextField fx:id="portField" alignment="CENTER" layoutX="260.0" layoutY="273.0" prefHeight="83.0" prefWidth="554.0" promptText="Insert Port" style="-fx-text-fill: #944110;">
        <effect>
            <InnerShadow offsetY="-10.0">
                <color>
                    <Color opacity="0.1410256326198578" />
                </color>
            </InnerShadow>
        </effect>
        <font>
            <Font name="Arial Black" size="24.0" />
        </font></TextField>
    <TextField fx:id="ipField" alignment="CENTER" layoutX="259.0" layoutY="147.0" prefHeight="83.0" prefWidth="554.0" promptText="Insert IP" style="-fx-text-fill: #944110;">
        <effect>
            <InnerShadow offsetY="-10.0">
                <color>
                    <Color opacity="0.1410256326198578" />
                </color>
            </InnerShadow>
        </effect>
        <font>
            <Font name="Arial Black" size="24.0" />
        </font>
    </TextField>

</AnchorPane>
